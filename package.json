{
   "name": "react_template",
   "version": "1.0.0",
   "description": "React template with Redity for states",
   "main": "index.js",
   "homepage": "https://github.com/Aventura-Tech",
   "scripts": {
      "test": "jest ./test",
      "test:watch": "jest --watch ./test",
      "build": "webpack --mode production",
      "lint": "standard --env",
      "watch": "webpack --watch --colors --mode development",
      "start": "webpack-dev-server --history--api-fallback --mode development"
   },
   "keywords": [
      "React",
      "Redity",
      "ERP Web Components",
      "Styled Components",
      "Js"
   ],
   "author": "Erick <ertriilive@gmail.com> (https://github.com/ertrii)",
   "license": "MIT",
   "dependencies": {
      "@aventura_tech/advance-erp-web-components": "^2.8.14",
      "@hot-loader/react-dom": "^16.11.0",
      "@reach/router": "^1.2.1",
      "axios": "^0.19.1",
      "moment": "^2.24.0",
      "prop-types": "^15.7.2",
      "react": "^16.12.0",
      "react-dom": "^16.12.0",
      "react-hot-loader": "^4.12.18",
      "redity": "^0.1.5",
      "serviceworker-webpack-plugin": "^1.0.1",
      "styled-components": "^5.0.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.8.0",
      "@babel/core": "^7.8.0",
      "@babel/plugin-proposal-class-properties": "^7.8.0",
      "@babel/plugin-proposal-export-default-from": "^7.8.3",
      "@babel/plugin-transform-runtime": "^7.8.0",
      "@babel/preset-env": "^7.8.0",
      "@babel/preset-react": "^7.8.0",
      "@babel/runtime": "^7.8.3",
      "@types/jest": "^24.0.25",
      "babel-eslint": "^10.0.3",
      "babel-jest": "^24.9.0",
      "babel-loader": "^8.0.6",
      "babel-plugin-styled-components": "^1.10.6",
      "copy-webpack-plugin": "^5.1.1",
      "dotenv": "^8.2.0",
      "file-loader": "^5.0.2",
      "html-webpack-plugin": "^3.2.0",
      "jest": "^24.9.0",
      "jest-transform-stub": "^2.0.0",
      "react-test-renderer": "^16.12.0",
      "standard": "^14.3.1",
      "uglifyjs-webpack-plugin": "^2.2.0",
      "url-loader": "^3.0.0",
      "webpack": "^4.41.5",
      "webpack-cli": "^3.3.10",
      "webpack-dev-server": "^3.10.1",
      "webpack-merge": "^4.2.2",
      "webpack-pwa-manifest": "^4.1.1"
   },
   "jest": {
      "transform": {
         "^.+\\.[t|j]sx?$": "babel-jest",
         ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
      },
      "setupFilesAfterEnv": [
         "./config/setupTests.js"
      ],
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ],
      "modulePathIgnorePatterns": [
         "./dist",
         "./public",
         "./vscode",
         "./build-utils",
         "./src",
         "./config"
      ],
      "transformIgnorePatterns": [
         "<rootDir>/node_modules/"
      ]
   },
   "standard": {
      "env": [
         "mocha",
         "jest"
      ]
   }
}
